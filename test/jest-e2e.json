{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@app/common": "<rootDir>/../libs/common/src",
    "@app/core/(.*)": "<rootDir>/../libs/core/src/$1",
    "@app/core": "<rootDir>/../libs/core/src",
    "@app/journal/(.*)": "<rootDir>/../libs/journal/src/$1",
    "@app/journal": "<rootDir>/../libs/journal/src",
    "@app/notification/(.*)": "<rootDir>/../libs/notification/src/$1",
    "@app/notification": "<rootDir>/../libs/notification/src",
    "@app/user-access-control/(.*)": "<rootDir>/../libs/user-access-control/src/$1",
    "@app/user-access-control": "<rootDir>/../libs/user-access-control/src",
    "@app/events/(.*)": "<rootDir>/../libs/events/src/$1",
    "@app/events": "<rootDir>/../libs/events/src",
    "@app/file-storage/(.*)": "<rootDir>/../libs/file-storage/src/$1",
    "@app/file-storage": "<rootDir>/../libs/file-storage/src",
    "@app/program/(.*)": "<rootDir>/../libs/program/src/$1",
    "@app/program": "<rootDir>/../libs/program/src",
    "@app/subscribe/(.*)": "<rootDir>/../libs/subscribe/src/$1",
    "@app/subscribe": "<rootDir>/../libs/subscribe/src"
  }
}