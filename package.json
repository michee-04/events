{
  "name": "evenements",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start:api": "nest start",
    "start:api:dev": "nest start --watch",
    "start:api:debug": "nest start --debug --watch",
    "start:api:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@digitaltg/cipher-cb": "^1.0.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/platform-socket.io": "^10.4.17",
    "@nestjs/platform-ws": "^10.4.17",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/websockets": "^10.4.17",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express-mongo-sanitize": "^2.2.0",
    "handlebars": "^4.7.8",
    "json-logic-js": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "nanoid": "^3.3.11",
    "nodemailer": "^6.10.0",
    "password-validator": "^5.3.0",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io-client": "^4.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "i": "^0.3.7",
    "jest": "^29.5.0",
    "npm": "^10.9.2",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/journal(|/.*)$": "<rootDir>/libs/journal/src/$1",
      "^@app/notification(|/.*)$": "<rootDir>/libs/notification/src/$1",
      "^@app/user-access-control(|/.*)$": "<rootDir>/libs/user-access-control/src/$1",
      "^@app/events(|/.*)$": "<rootDir>/libs/events/src/$1",
      "^@app/file-storage(|/.*)$": "<rootDir>/libs/file-storage/src/$1",
      "^@app/program(|/.*)$": "<rootDir>/libs/program/src/$1"
    }
  }
}